@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<h3>Claims</h3>
<table class="table">
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }    
</table>

<h3>Request headers</h3>
<table class="table">
    @foreach (var header in Context.Request.Headers)
    {
        <tr>
            <td>@header.Key</td>
            <td>@header.Value</td>
        </tr>
    }    
</table>

